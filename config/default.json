{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "metrics": {},
    "tracing": {
      "isEnabled": false
    },
    "shared": {},
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": 8080,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "users": [],
  "audit-db": {
    "type": "postgres",
    "host": "127.0.0.1",
    "port": 5433,
    "username": "postgres",
    "password": "postgres",
    "database": "audit-logs",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  },
  "extractable-db": {
    "type": "postgres",
    "host": "127.0.0.1",
    "port": 5434,
    "username": "postgres",
    "password": "postgres",
    "database": "extractable-records",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
