{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "metrics": {},
    "tracing": {
      "isEnabled": false
    },
    "shared": {},
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": 8080,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "pagination": {
    "maxRecords": 10000
  },
  "externalServices": {
    "catalog": "http://127.0.0.1:8080",
    "publicExtractableRoutes": [{ "url": "https://linl-to-env1" }, { "url": "https://linl-to-env2" }]
  },
  "users": [{ "username": "username", "password": "password" }],
  "db": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "3d-extractable-management",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
